@model RuntagViewModel
@{
    
}
<form asp-controller="Runtag" asp-action="Create">
    <div class="form-group">
        <label asp-for="Title"></label>
        <input class="form-control" asp-for="Title" />
        <span asp-validation-for="Title" class="text-danger"></span>
    </div>
    
    <div class="form-group">
        <label asp-for="Address"></label>
        <input class="form-control" asp-for="Address" />
        <span asp-validation-for="Address" class="text-danger"></span>
    </div>
    
    <div class="form-group">
        <label asp-for="Description"></label>
        <input class="form-control" asp-for="Description" />
        <span asp-validation-for="Description" class="text-danger"></span>
    </div>
    
    <div class="form-group">
        <label asp-for="CustomerName"></label>
        <input class="form-control" asp-for="CustomerName" />
        <span asp-validation-for="CustomerName" class="text-danger"></span>
    </div>
    
    <div class="form-group">
        <label asp-for="DueTime"></label>
        <input class="form-control" asp-for="DueTime" />
        <span asp-validation-for="DueTime" class="text-danger"></span>
    </div>    
    
    <button type="submit" class="btn btn-primary">Opret</button>   
</form>
